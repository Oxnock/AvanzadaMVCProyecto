@model IEnumerable<ProyectoIdentity.Models.Parametros>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.fechaLimOrd)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.fechaLimExt)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.descuento)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.maxCred)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.fechaLimOrd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fechaLimExt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.descuento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.maxCred)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ParametrosId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ParametrosId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ParametrosId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
