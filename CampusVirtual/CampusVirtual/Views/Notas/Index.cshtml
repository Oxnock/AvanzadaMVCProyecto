@model IEnumerable<CampusVirtual.Model.Entities.Nota>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Usuario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.Evaluacion.Curso)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Evaluacion.Nombre)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Evaluacion.Notas)
			</td>
		</tr>
}
    </tbody>
</table>
